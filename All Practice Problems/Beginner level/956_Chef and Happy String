t = int(input())
for i in range(t):
    s = input()
    l = ['a','e','i','o','u']
    count = 0
    for i in range(len(s)-2):
        if s[i] in l:
            if s[i+1] in l and s[i+2] in l:
                count = 1 
                break
    if count==1:
        print('Happy')
    else:
        print('Sad')
